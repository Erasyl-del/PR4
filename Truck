public class Truck implements IVehicle {
    private double capacity;
    private int axles;

    public Truck(double capacity, int axles) {
        this.capacity = capacity;
        this.axles = axles;
    }

    @Override
    public void drive() {
        System.out.println("Грузовик грузоподъемностью " + capacity + " т едет.");
    }

    @Override
    public void refuel() {
        System.out.println("Грузовик с " + axles + " осями заправляется.");
    }
}
